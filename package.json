    "filepond-plugin-image-exif-orientation": "^1.0.11",
    "filepond-plugin-image-preview": "^4.6.10",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "nodemailer": "^6.6.3",
    "react": "^17.0.2",
    "react-charts": "^2.0.0-beta.7",
    "react-csv": "^2.0.3",
    "react-data-table-component": "^7.0.0",
